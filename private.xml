<?xml version="1.0"?>
<root>
  <item>
    <name>Use Command as FN of HHK</name>
    <item>
      <name>Command + semi colon to left</name>
      <identifier>private.command_and_semicolon_to_left</identifier>
      <autogen>
        --KeyToKey--
        KeyCode::SEMICOLON, ModifierFlag::COMMAND_L, 
        KeyCode::CURSOR_LEFT
      </autogen>
    </item>
    <item>
      <name>Command + quote to right</name>
      <identifier>private.command_and_quote_to_right</identifier>
      <autogen>
        --KeyToKey--
        KeyCode::QUOTE, ModifierFlag::COMMAND_L, 
        KeyCode::CURSOR_RIGHT
      </autogen>
    </item>
    <item>
      <name>Command + [ to up</name>
      <identifier>private.command_and_[_to_up</identifier>
      <autogen>
        --KeyToKey--
        KeyCode::BRACKET_LEFT, ModifierFlag::COMMAND_L, 
        KeyCode::CURSOR_UP
      </autogen>
    </item>
    <item>
      <name>Command + / to down</name>
      <identifier>private.command_and_/_to_down</identifier>
      <autogen>
        --KeyToKey--
        KeyCode::SLASH, ModifierFlag::COMMAND_L, 
        KeyCode::CURSOR_DOWN
      </autogen>
    </item>
    <item>
      <name>Command + K to home</name>
      <identifier>private.command_and_k_to_home</identifier>
      <autogen>
        --KeyToKey--
        KeyCode::K, ModifierFlag::COMMAND_L,
        KeyCode::CURSOR_LEFT, ModifierFlag::CONTROL_L
      </autogen>
    </item>
    <item>
      <name>Command + , to end</name>
      <identifier>private.command_and_comma_to_end</identifier>
      <autogen>
        --KeyToKey--
        KeyCode::COMMA, ModifierFlag::COMMAND_L,
        KeyCode::CURSOR_RIGHT, ModifierFlag::CONTROL_L
      </autogen>
    </item>
    <item>
      <name>Command + ` to delete</name>
      <identifier>private.command_and_backquote_to_delete</identifier>
      <autogen>
        --KeyToKey--
        KeyCode::BACKQUOTE, ModifierFlag::COMMAND_L,
        KeyCode::FORWARD_DELETE
      </autogen>
    </item>
  </item>
</root>
